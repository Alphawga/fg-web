{
  "name": "schoolwave",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "next dev --port 8079",
    "build": "next build",
    "start": "next start --port 8079",
    "lint": "next lint --fix",
    "seed": "prisma db seed -- --environment development",
    "seed:prod": "prisma db seed -- --environment production",
    "generate": "prisma generate",
    "migrate": "prisma migrate dev",
    "migrate:status": "prisma migrate status",
    "migrate:reset": "prisma migrate reset",
    "migrate:deploy": "prisma migrate deploy",
    "type-check": "tsc --noemit",
    "infra": "docker compose -p schoolwave up -d",
    "infra:down": "docker compose -p schoolwave down -v",
    "test": "jest",
    "cypress:install": "cypress install",
    "cypress:open": "cypress open",
    "e2e": "cypress run --e2e",
    "component": "cypress open --component",
    "component:headless": "cypress run --component"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.3",
